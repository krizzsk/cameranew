<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ffd9e0eb" android:endColor="#ffd9e0eb" android:angle="0" android:centerY="0.75" android:centerColor="#ffd9e0eb"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#ff3c97ff" android:endColor="#ff3c97ff" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
