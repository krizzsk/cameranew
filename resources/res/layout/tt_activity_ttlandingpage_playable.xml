<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
        <com.bytedance.sdk.openadsdk.core.widget.webview.SSWebView android:id="@+id/tt_browser_webview" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <ProgressBar android:layout_gravity="top|center_horizontal" android:id="@+id/tt_browser_progress" android:layout_width="match_parent" android:layout_height="3dp" android:progress="1" android:progressDrawable="@drawable/tt_browser_progress_style" style="@android:style/Widget.ProgressBar.Horizontal"/>
        <com.bytedance.sdk.openadsdk.core.widget.webview.SSWebView android:id="@+id/tt_browser_webview_loading" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <RelativeLayout android:gravity="center" android:layout_gravity="end|top" android:id="@+id/tt_playable_ad_close_layout" android:background="@drawable/tt_mute_btn_bg" android:visibility="gone" android:layout_width="28dp" android:layout_height="28dp" android:layout_marginTop="20dp" android:layout_marginRight="24dp" android:layout_marginEnd="24dp">
            <ImageView android:id="@+id/tt_playable_ad_close" android:visibility="visible" android:layout_width="12dp" android:layout_height="12dp" android:src="@drawable/tt_video_close_drawable"/>
        </RelativeLayout>
    </FrameLayout>
</LinearLayout>
